{
    "manifest_version": 2,
    "name": "Grun",
    "description": "This extension converts the text in any page to Grun.",
    "version": "1.0",
    "author": "Steven A. Dunn and Brandon Giesing",
    "browser_action": {
        "default_icon": "grun.png"
    },
    "content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*", "https://twitter.com/*/*"],
        "js": ["grun.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
